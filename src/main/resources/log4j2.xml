<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<!-- Console Appender -->
		<Console name="ConsoleJson" target="SYSTEM_OUT" follow="true">
			<JsonLayout complete="false" compact="true" eventEol="true" properties="true"/>
		</Console>
	</Appenders>

	<Loggers>
		<Root level="info">
			<AppenderRef ref="ConsoleJson" />
		</Root>
	</Loggers>
</Configuration>